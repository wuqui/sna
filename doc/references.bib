	Publisher = {Cambridge University Press},
	Title = {Modeling language change: An evaluation of Trudgill's theory of the emergence of New Zealand English},
	Url = {https://www.cambridge.org/core/article/div-class-title-modeling-language-change-an-evaluation-of-trudgillandapos-s-theory-of-the-emergence-of-new-zealand-english-div/0264FEC709F3CCFDC2CBCED0F3408230},
	Volume = {21},
